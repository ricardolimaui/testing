#!/Users/joseabrantes/Documents/Dev-Corner/docs/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'watchdog==2.1.6','console_scripts','watchmedo'
__requires__ = 'watchdog==2.1.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('watchdog==2.1.6', 'console_scripts', 'watchmedo')()
    )
